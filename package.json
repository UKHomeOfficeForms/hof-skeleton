{
  "name": "hof-boilerplate-form",
  "version": "1.0.0",
  "description": "Simple standard form made using HOF",
  "main": "index.js",
  "engines": {
    "node": "^18.12.1"
  },
  "scripts": {
    "start": "node server.js",
    "start:dev": "hof-build watch --env",
    "build": "hof-build",
    "postinstall": "hof-build",
    "test": "NODE_ENV=test yarn run test:lint && yarn run test:unit",
    "test:unit": "nyc _mocha \\\"tests/unit/**/*.spec.js\\\"",
    "test:lint": "eslint . --config ./node_modules/eslint-config-hof/default.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/UKHomeOffice/coa.git"
  },
  "author": "HOF",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/UKHomeOfficeForms/hof-skeleton/issues"
  },
  "homepage": "https://github.com/UKHomeOfficeForms/hof-skeleton/",
  "mocha": {
    "reporter": "spec",
    "require": "test/setup.js",
    "recursive": "true",
    "timeout": "6000"
  },
  "dependencies": {
    "accessible-autocomplete": "^2.0.4",
    "bl": "^6.0.12",
    "busboy": "^1.6.0",
    "hof": "^20.5.6",
    "jquery": "^3.6.3",
    "lodash": "^4.17.21",=
  },
  "devDependencies": {
    "eslint": "^8.56.0",
    "eslint-config-hof": "^1.3.4",
    "mocha": "^10.6.0",
    "nyc": "^15.1.0"
  }
}
