{
  "name": "hof-project",
  "version": "1.0.0",
  "main": "index.js",
  "description": "hof-skeleton is a boiler-plate for any HOF project.",
  "engines": {
    "node": ">=18.12.0"
  },
  "scripts": {
    "start": "node server.js",
    "start:dev": "hof-build watch --env",
    "test": "NODE_ENV=test yarn run test:lint && yarn run test:unit",
    "test:lint": "eslint . --config ./node_modules/eslint-config-hof/default.js",
    "build": "hof-build",
    "postinstall": "hof-build"
  },
  "dependencies": {
    "accessible-autocomplete": "^2.0.4",
    "axios": "^1.6.5",
    "bl": "^6.0.10",
    "busboy": "^1.6.0",
    "bytes": "^3.1.2",
    "dotenv": "^16.3.1",
    "eslint-config-hof": "^1.3.1",
    "hof": "^20.3.5",
    "jquery": "^3.7.1",
    "lodash": "^4.17.21",
    "moment": "^2.30.1",
    "node-cron": "^3.0.3"
  },
  "devDependencies": {
    "eslint": "^7.32.0",
    "eslint-config-hof": "^1.3.3",
    "mocha": "^10.2.0",
    "proxyquire": "^2.1.3",
    "reqres": "^3.0.1",
    "sinon": "^15.1.0",
    "sinon-chai": "^3.7.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/UKHomeOfficeForms/hof-skeleton.git"
  },
  "author": "Temitope Ayoku & SAS HOF Team",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/UKHomeOfficeForms/hof-skeleton/issues"
  },
  "homepage": "https://github.com/UKHomeOfficeForms/hof-skeleton#readme",
  "mocha": {
    "reporter": "spec",
    "require": "test/common.js",
    "recursive": "true",
    "timeout": "9000",
    "exit": "true"
  }
}
